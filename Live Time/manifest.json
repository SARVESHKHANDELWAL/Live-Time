{
    "name": "Live Time",
    "version": "1.0",
    "description": "Build an Extension!",
    "manifest_version": 2,
    "icons":{"128":"a128.png",
            "48":"a48.png",
            "16":"a16.png"
          },
    "content_scripts":[
      {
        "matches":["*://*facebook*"],
        "js":["content.js"]
      }
    ],
    ,
  "background":{
    "scripts":["background.js"]
  },
    "browser_action":{
          "default_icon":"icon16.png",
          "default_popup":"popup/popup.html",
          "default_title":"Live Time"
        }

}
